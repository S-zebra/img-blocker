{
  "name": "IMG Blocker",
  "version": "0.1",
  "description": "Shuts out images on the website",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["contentSettings", "tabs"],
  "icons": {
    "128": "resources/block/128.png"
  },
  "browser_action": {}
}